#include "BitFlipProb.h"
using namespace std;
#include <string>

// The mutate function in this class goes through every digit in the binary string and “flips” each of the binary digit with probability p. 
string BitFlipProb::mutator(Individual* i, int k){
    string out=i->getString();
    int length=i->getLength();
    int a = k%length-1;
    string s="";
    s+=out[a];
    return s;
    
    
}
