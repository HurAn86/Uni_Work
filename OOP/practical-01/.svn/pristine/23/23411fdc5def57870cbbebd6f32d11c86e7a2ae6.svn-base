#include <iostream>
#include <stdlib.h>
void twofivenine(int array[], int n){
    int count2 = 0;
    int count5 = 0;
    int count9 = 0;
    for(int i = 0;i < n; i++){
        switch(array[i]){
            case 2:
                if(array[i] == 2){
                    count2++;
                }
            case 5:
                if(array[i] == 5){
                    count5++;
                }
            case 9:
                if(array[i] == 9){
                    count9++;
                }
        }
    }
    std::cout << "2: " << count2 << "; 5: "<< count5 << "; 9: "<< count9 << std::endl;
}
